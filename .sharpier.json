{
  "space_after_keywords_in_control_flow_statements": true,
  "indent_width": 2,
  "max_line_length": 80,
  "align_collection_initializer_values": true,
  "space_before_opening_brace": false,
  "trailing_comma_in_collection_initializers": true,
  "no_trailing_whitespace": true,
  "insert_final_newline": true,
  "indent_case_contents": true,
  "indent_switch_labels": true,
  "preserve_comments": true
}
